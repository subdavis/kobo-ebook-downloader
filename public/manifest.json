{
  "name": "Kobo Ebook Downloader",
  "short_name": "Kobodl",
  "version": "2020.1.1",
  "manifest_version": 2,
  "minimum_chrome_version": "48",
  "description": "Download books from kobo",
  "key": "",
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Kobodl"
  },
  "permissions": [
    "storage",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "https://authorize.kobo.com/",
    "http://store.kobobooks.com/",
    "https://store.kobobooks.com/",
    "https://storeapi.kobo.com/",
    "https://www.kobo.com/",
    "https://auth.kobobooks.com/"
  ],
  "externally_connectable": {
    "matches": ["*://*.kobo.com/*"]
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  }
}
